{
  "redirects": [
    {
      "source": "/auth/callback",
      "destination": "/auth-callback.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ],
  "functions": {
    "api/bexar.py": {
      "runtime": "python3.11",
      "memory": 1024,
      "maxDuration": 30
    }
  }
}
